{% extends "../layout.njk" %}
{% from "govuk/components/button/macro.njk" import govukButton %}

{% block formContent %}
  <table class="govuk-table">
    <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th scope="col" class="govuk-table__header govuk-!-width-one-quarter">Field</th>
      <th scope="col" class="govuk-table__header govuk-!-width-three-quarters">Value</th>
    </tr>
    </thead>
    <tbody class="govuk-table__body">
    {% for key, value in oasysEquivalent %}
      <tr class="govuk-table__row">
        <th scope="row" class="govuk-table__header">{{ key }}</th>
        <td class="govuk-table__cell">{{ value | dump }}</td>
      </tr>
    {% endfor %}
    </tbody>
  </table>
{% endblock %}
